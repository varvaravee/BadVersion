// The API isBadVersion is defined for you.
// bool isBadVersion(int version);

class Solution {
public:
    int firstBadVersion(int n) {
        int badVersion=0;
        for (int i=1; i<=n; i++  )
        {
            if (isBadVersion(i))
            {
                badVersion=i;
                break;
            }
        }
        return badVersion;
    }
};
